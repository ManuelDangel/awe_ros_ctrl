<launch>
  <node name="awe_nmpc" pkg="awe_ctrl" type="awe_nmpc" output="screen"/>

    <param name="/nmpc/loop_rate" type="double" value="10.0" />
    <param name="/nmpc/time_step" type="double" value="0.1" />
    <param name="/nmpc/coordinate_flip" type="bool" value="false" />
    <param name="/nmpc/fake_signals" type="bool" value="false" />

    <param name="/nmpc/param_vw" type="double" value="5.0" />
    <param name="/nmpc/param_r" type="double" value="100.0" />
    <param name="/nmpc/param_r_dot" type="double" value="0.0" />
    <param name="/nmpc/param_circle_azimut" type="double" value="0.0" />
    <param name="/nmpc/param_circle_elevation" type="double" value="1.04" />
    <param name="/nmpc/param_circle_angle" type="double" value="0.29" />
    <param name="/nmpc/param_m" type="double" value="2.0" />
    <param name="/nmpc/param_cla" type="double" value="0.2" />
    <param name="/nmpc/param_cld" type="double" value="0.025" />
    <param name="/nmpc/param_phi_freq" type="double" value="5.0" />
    <param name="/nmpc/param_wind_azimut" type="double" value="0.0" />
    <param name="/nmpc/param_thrust_power" type="double" value="0.0" />
    <param name="/nmpc/param_weight_tracking" type="double" value="1.0" />
    <param name="/nmpc/param_weight_power" type="double" value="1.0" />

    <param name="/nmpc/angle_of_attack_deg" type="double" value="3.0" />

    <param name="/nmpc/cost_control" type="double" value="1.0" />
    <param name="/nmpc/cost_tracking" type="double" value="100.0" />
    <param name="/nmpc/cost_power" type="double" value="0.0" />

    <param name="/nmpc/thrust_max" type="double" value="0.0" />
    <param name="/nmpc/thrust_0_speed" type="double" value="20.0" />
    <param name="/nmpc/thrust_p" type="double" value="0.1" />

</launch>
